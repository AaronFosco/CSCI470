import java.io.*;

public class CheckItentifiers
{
 public static void main(String[] args) throws IOException
 { 
   String itent;
   System.out.println("Please enter a vaild java identifier: ");
   for (int i = 0; i < 5; i++)
   {
     System.in.read(itent);
   }
 }  
}